  "scripts": {
    "install:app": "cd app && npm install",
    "postinstall": "npm run install:app",
    "start": "cd app && npm start",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run --headless"
